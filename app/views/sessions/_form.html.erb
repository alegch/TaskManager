<%= form_tag session_path do %>
  <div class="field">
    <%= label_tag :email %>
    <%= text_field_tag :email, params[:email] %>
  </div>
  <div class="field">
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>
  <div class="actions"><%= submit_tag "Log in" %></div>
<% end %>

<p class="text-info"> Don't have account?
  <%= link_to "Sign Up", new_user_path, :class => "btn" %>
</p>
